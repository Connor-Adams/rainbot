{
  "name": "rainbot",
  "version": "1.0.0",
  "description": "the rinabot discord bot",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build:ts": "tsc",
    "start": "npm run build:ts && node index.js",
    "dev:ui": "node dev-server.js",
    "build:ui": "cd ui && npm install && npm run build",
    "build": "npm run build:ui && npm run start",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "[ -d .git ] && husky || true",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.9",
    "async-mutex": "^0.5.0",
    "connect-redis": "^7.1.1",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "express-session": "^1.18.1",
    "inversify": "^6.0.2",
    "libsodium-wrappers": "^0.7.15",
    "opusscript": "^0.1.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.11.3",
    "play-dl": "^1.9.7",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "session-file-store": "^1.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "youtube-dl-exec": "^3.0.28",
    "express-rate-limit": "^8.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-oauth2": "^1.4.17",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
