{
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@std/": "https://deno.land/std@0.224.0/",
    "@std/path": "https://deno.land/std@0.224.0/path/mod.ts",
    "@std/assert": "https://deno.land/std@0.224.0/assert/mod.ts",
    "@utils/": "./utils/",
    "@components": "./components/",
    "@types": "./types/",
    "@handlers": "./handlers/",
    "@events": "./events/",
    "@server": "./server/",
    "@/": "./ui/src/",
    "discord.js": "npm:discord.js@14.14.1",
    "@discordjs/voice": "npm:@discordjs/voice@0.17.0",
    "@discordjs/opus": "npm:@discordjs/opus@0.10.0",
    "express": "npm:express@5.2.1",
    "dotenv": "npm:dotenv@16.4.5",
    "winston": "npm:winston@3.19.0",
    "pg": "npm:pg@8.11.3",
    "redis": "npm:redis@4.7.0",
    "passport": "npm:passport@0.7.0",
    "passport-oauth2": "npm:passport-oauth2@1.8.0",
    "express-session": "npm:express-session@1.18.2",
    "connect-redis": "npm:connect-redis@7.1.1",
    "multer": "npm:multer@2.0.2",
    "play-dl": "npm:play-dl@1.9.7",
    "async-mutex": "npm:async-mutex@0.5.0",
    "spotify-web-api-node": "npm:spotify-web-api-node@5.0.2",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@3.965.0",
    "process": "node:process",
    "buffer": "node:buffer",
    "fs": "node:fs",
    "path": "node:path",
    "crypto": "node:crypto",
    "url": "node:url",
    "querystring": "node:querystring",
    "events": "node:events",
    "util": "node:util",
    "zlib": "node:zlib",
    "http": "node:http",
    "https": "node:https",
    "os": "node:os",
    "child_process": "node:child_process",
    "stream": "node:stream"
  },
  "tasks": {
    "dev": "deno task dev:raincloud & deno task dev:rainbot & deno task dev:pranjeet & deno task dev:hungerbot",
    "dev:raincloud": "deno run --allow-all --env --watch apps/raincloud/index.ts",
    "dev:rainbot": "deno run --node-modules-dir=auto --allow-all --env --watch apps/rainbot/src/index.ts",
    "dev:pranjeet": "deno run --node-modules-dir=auto --allow-all --env --watch apps/pranjeet/src/index.ts",
    "dev:hungerbot": "deno run --node-modules-dir=auto --allow-all --env --watch apps/hungerbot/src/index.ts",
    "build:ui": "deno run --node-modules-dir=auto --allow-read --allow-run --allow-net scripts/build-ui.ts",
    "deploy:validate": "deno task lint && deno task type-check && deno task test",
    "test:ci": "deno test --coverage=coverage/ --allow-all"
  }
}
