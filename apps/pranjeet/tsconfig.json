{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "../../",
    "noImplicitReturns": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "baseUrl": "../../",
    "paths": {
      "@rainbot/protocol": ["packages/protocol/dist/index.d.ts"],
      "@rainbot/protocol/*": ["packages/protocol/dist/*"],
      "@rainbot/shared": ["packages/shared/dist/index.d.ts"],
      "@rainbot/shared/*": ["packages/shared/dist/*"],
      "@utils/*": ["utils/*"],
      "@voice/*": ["utils/voice/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
