{
  "name": "@rainbot/utils",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "yarn clean && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:ts": "yarn clean && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "@rainbot/protocol": "*",
    "async-mutex": "^0.5.0",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5",
    "pg": "^8.13.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/pg": "^8.11.10",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.9.3"
  }
}
