    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@rainbot/types": "*",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  }
