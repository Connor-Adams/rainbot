{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["ES2022"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowJs": false,
    "noEmit": false,
    "types": ["node"],
    "moduleDetection": "force",
    "declaration": false,
    "sourceMap": false,
    "outDir": "./dist",
    "rootDir": "./"
  },
  "include": [
    "commands/**/*.ts",
    "utils/**/*.ts",
    "types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "ui",
    "dist"
  ]
}
